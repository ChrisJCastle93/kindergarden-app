<section>
  <div class="flex flex-col">
    <br />
    <h5 class="mb-2 text-2xl font-bold tracking-tight text-gray-900">History</h5>
    <br />
    <div class="overflow-x-auto sm:-mx-6 lg:-mx-8">
      <div class="inline-block py-2 w-full sm:px-6 lg:px-8">
        <div class="overflow-hidden shadow-md sm:rounded-lg">
          <table class="w-full">
            <thead class="bg-gray-50">
              <tr>
                <th scope="col" class="py-3 px-6 text-xs font-medium tracking-wider text-left text-gray-700 uppercase">
                  Child
                </th>
                <th scope="col" class="py-3 px-6 text-xs font-medium tracking-wider text-left text-gray-700 uppercase">
                  Group
                </th>
                <th scope="col" class="py-3 px-6 text-xs font-medium tracking-wider text-left text-gray-700 uppercase">
                  Disease
                </th>
                <th scope="col" class="py-3 px-6 text-xs font-medium tracking-wider text-left text-gray-700 uppercase">
                  Test name
                </th>
                <th scope="col" class="py-3 px-6 text-xs font-medium tracking-wider text-left text-gray-700 uppercase">
                  Disease
                </th>
                <th scope="col" class="py-3 px-6 text-xs font-medium tracking-wider text-left text-gray-700 uppercase">
                  Date
                </th>
                <th scope="col" class="py-3 px-6 text-xs font-medium tracking-wider text-left text-gray-700 uppercase">
                  Result
                </th>
              </tr>
            </thead>
            <tbody>
              {{#each historyOfTests}}
                <tr class="bg-white border-b">
                  <td class="py-4 px-6 text-sm font-medium text-gray-900 whitespace-nowrap">
                    {{this.name}}
                  </td>
                  <td class="py-4 px-6 text-sm text-gray-500 whitespace-nowrap">
                    {{this.group}}
                  </td>
                  <td class="py-4 px-6 text-sm text-gray-500 whitespace-nowrap">
                    {{this.disease}}
                  </td>
                  <td class="py-4 px-6 text-sm text-gray-500 whitespace-nowrap">
                    {{this.test}}
                  </td>
                  <td class="py-4 px-6 text-sm text-gray-500 whitespace-nowrap">
                    {{this.date}}
                  </td>
                  <td class="py-4 px-6 text-sm text-gray-500 whitespace-nowrap">
                    {{this.disease}}
                  </td>
                  <td class="py-4 px-6 text-sm text-gray-500 whitespace-nowrap">
                    {{#if this.result}}
                      <span class="bg-red-100 text-red-800 text-xs font-semibold mr-2 px-2.5 py-0.5 rounded">POSITIVE</span>
                    {{else}}
                      <span class="bg-green-100 text-green-800 text-xs font-semibold mr-2 px-2.5 py-0.5 rounded">NEGATIVE</span>
                    {{/if}}
                  </td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>

</section>
